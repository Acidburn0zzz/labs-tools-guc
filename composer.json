{
	"autoload": {
		"psr-4": {
			"Guc\\": "src/"
		}
	},
	"config": {
		"classmap-authoritative": true,
		"optimize-autoloader": true
	},
	"require": {
		"php": ">=5.4",
		"Krinkle/toollabs-base": "^0.8.1",
		"krinkle/intuition": "^0.5.1"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"squizlabs/php_codesniffer": "^2.7.1"
	},
	"scripts": {
		"test": [
			"parallel-lint . --exclude vendor",
			"phpcs -p -s"
		]
	}
}
